{
  "name": "api",
  "version": "1.0.0",
  "description": "backend API for shopstack",
  "main": "src/index.js",
  "engines": {
    "node": "11.9.0",
    "npm": "6.9.0"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf ./dist && babel src -d ./dist --copy-files && npm run migrate",
    "start": "node ./dist/index.js",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "migrate": "babel-node ./src/database/runMigrations.js",
    "migrate:undo": "babel-node ./src/database/revertMigrations.js",
    "test": "export NODE_ENV=test && npm run migrate && nyc mocha --recursive ./test/**/*spec.js --exit --require @babel/register && npm run migrate:undo",
    "coverage": "nyc report --reporter=lcov"
  },
  "keywords": [
    "node.js",
    "express.js",
    "api"
  ],
  "author": "Ovie Okeh",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "redis": "^2.8.0",
    "short-uuid": "^3.1.1",
    "stripe": "^7.1.0",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.0",
    "expect.js": "^0.3.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1"
  }
}
